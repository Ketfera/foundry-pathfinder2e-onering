":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"-","type":"Number","label":"Bulk"},"equippedBulk":{"value":""},"price":{"value":"0","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":false},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":null,"type":"Number","label":"Item Level"},"property1":{"value":"","dice":null,"die":"","damageType":"","type":"String","label":"Primary Property","strikeConditionType":"","strikeConditionValue":null,"critDice":null,"critDie":"","critDamageType":"","criticalConditionType":"","criticalConditionValue":null},"property2":{"value":"","dice":0,"die":"","damageType":"","type":"String","label":"Secondary Property"},"property3":{"value":"","dice":0,"die":"","damageType":"","type":"String","label":"Tertiary Property"},"weaponType":{"value":"martial","type":"String","label":"Weapon Category"},"group":{"value":"sling","type":"String","label":"Weapon Group"},"hands":{"value":"","type":"String","label":"Hands"},"bonus":{"value":0,"type":"String","label":"Item Bonus"},"damage":{"value":"","dice":1,"die":"d6","damageType":"bludgeoning","type":"String","label":"damage"},"bonusDamage":{"value":0},"range":{"value":"60","type":"String","label":"Weapon Range"},"reload":{"value":"1","type":"String","label":"Reload"},"ability":{"value":"dex","type":"String","label":"Offensive Ability"},"MAP":{"value":""},"potencyRune":{"value":""},"strikingRune":{"value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""}},"sort":2925001},{"_id":"J4xFjQJCXEYob6Zn","flags":{"_sheetTab":"details"},"name":"